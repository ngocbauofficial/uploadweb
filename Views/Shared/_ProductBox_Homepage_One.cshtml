                    <a href="@item.SeName"><img src="@item.DefaultPictureModel.ImageUrl" title="@item.DefaultPictureModel.Title" alt="@item.DefaultPictureModel.AlternateText" class="img-fluid"></a>
                </div>
                <div class="mo-ta" data-aos="fade-up" data-aos-delay="500" data-aos-duration="500">
                    <a href="@item.SeName">@item.Name</a>
                    <a href="@item.SeName">@item.ShortDescription</a>
                </div>
            </div>
