@model IList<ProductDetailsModel.ProductAttributeModel>
    @using Nop.Web.Models.Catalog;
@if (Model.Count() > 0)
{
    <label for="">Phiên Bản:</label>
    <select class="main" id="version">
        @foreach (var item in Model)
        {
            foreach (var item1 in item.Values)
            {
                <option value="@item1.PriceAdjustmentValue">@item1.Name (@item1.PriceAdjustment)</option>
            }
        }
    </select>
}
